//오픈소스시간에 수행한 파이썬 코딩도장의 회문판별 문제입니다.
class NotPalindromeError(Exception):
    def __init__(self):
        super().__init__('회문이 아닙니다.')
    
    
def palindrome(word):
    if list(word)==list(reversed(word)):
        print(word)
    else:
        raise NotPalindromeError
        
try:
    word = input()
    palindrome(word)
except NotPalindromeError as e:
    print(e)
